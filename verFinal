quadrado_apagar: #fazer quadrado
	beq $20, $21, voltar
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -508
	addi $21, $21, 1
	j quadrado_apagar
v_goomba:
	beq $16, $0, m_goombaTras
	j m_goombaFrente
r_goomba:
	addi $18, $0, 0
	addi $16, $0, 0
	j verificarKey
pointb:
	addi $18, $0, 0
	j m_goombaFrente
m_goombaFrente:
	addi $20, $0, 32
	beq $20, $18, r_goomba ######
	addi $16, $0, 1
	addi $19, $17, 4
	addi $18, $18, 1
	addi $4, $0, 150
	addi $2, $0, 32
	syscall
	beq $3, 1, apagar_goombavivo1
	beq $3, 2, apagar_lastgoomba
	j apagar_goomba
m_goombaTras:
	addi $20, $0, 32
	beq $20, $18, pointb
	addi $16, $0, 0 #flag que verifica se ele vai para tras ou frente lï¿½ em cima
	addi $19, $17, -4
	addi $18, $18, 1
	addi $4, $0, 150
	addi $2, $0, 32
	syscall
	beq $3, 1, apagar_goombavivo1_2
	j apagar_goomba2
