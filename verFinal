	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
