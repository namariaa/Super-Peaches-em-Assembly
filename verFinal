	addi $8, $8, 16
	addi $20, $0, 2
	addi $21, $0, 0
	jal barra_apagar_invertida3
	addi $8, $8, -1568 #rosto
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_apagar_invertida3
	addi $8, $8, -1048
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_apagar_invertida3
	addi $8, $8, 496 #olho
	addi $10,$0, 0x4d6df3
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 508
	addi $10,$0, 0x00b7ef
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $20, $0, 6 #comeï¿½a o cabelo a partir daqui
	addi $10, $0, 0xffc20e
	addi $21, $0, 0
	addi $8, $8, -1548
	jal barra_apagar_invertida3
	addi $20, $0, 4
