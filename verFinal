	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, -508
	sw $10, 0($8)
	addi $8, $8, -516
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $10, $0, 0x000000
