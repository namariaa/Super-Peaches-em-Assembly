	jal floco
	addi $8, $8, 4000
	jal floco
	addi $8, $8, 128
	jal floco
	addi $8, $8, 128
	jal floco
	addi $8, $8, -1000
	jal floco
	j verificarKey2
floco:
	sw $10, 0($8)
	addi $8, $8, 516
	sw $10, 0($8)
	addi $8, $8, -8
	sw $10, 0($8)
	addi $8, $8, -1024
	sw $10, 0($8)
	addi $8, $8, 8
	sw $10, 0($8)
	addi $8, $8, 508
	jr $31
	
verificarKey2:
	lui $24, 0x1001 #se chegar na bandeira vai para a fase2
	addi $24, $24, 27588
	lw $25, 0($24)
	addi $10, $0, 0x8c4bb7
	beq $25, $10, m92 #rotulo para inciar a animação da bandeira subindo
	lui $24, 0x1001 #se chegar na parede vai para a fase2 de novo
	addi $24, $24, 26624
	lw $25, 0($24)
	addi $10, $0, 0xff80ab
	beq $25, $10, start_1    
	addi $10, $0, 0x8c4bb7
	beq $25, $10, start_1 
	addi $24, $19, -2580 #se houver colisão com o koopa
	lw $25, 0($24)
	addi $10, $0,0xff80ab
	beq $25, $10, morreu
	addi $24, $19, 0 #se houver colisão com o koopa
	lw $25, 0($24)
	addi $10, $0, 0xff80ab
	beq $25, $10, morreu
	lui $1, 0xffff
	lw $24, 4($1) #se for diferente de 0 algo foi digitado
	beq $24, $0, whileTrue2 #se nï¿½o foi digitado nada
	addi $25, $0, 'd'
	beq $24, $25, moverFrente2
	addi $25, $0, 'a'
	beq $24, $25, moverTras2
	addi $25, $0, 'w'
	beq $24, $25, moverCima2
	addi $25, $0, ' '
	beq $24, $25,whileTrue2
	addi $25, $0, 's'
	beq $24, $25,entrarcano
moverFrente2:
	sw $0, 4($1)#para ele caminhar só uma vez, colocar na região de memória do $1 o valor zero para não ficar caminhando infinitamente
	addi $23, $23, 12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar2
moverTras2:
	sw $0, 4($1)
	addi $23, $23, -12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar_invertida2
moverCima2:
	sw $0, 4($1)
	addi $23, $23, -11264 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	jal apagar_pulo2
moverBaixo2:
	addi $23, $23, 11276 #Aumenta aqui para pular mais alto lembrando que esse ponto é 12 a mais que o moverCima
	addi $4, $0, 550 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
	jal apagar_descer2
entrarcano:
	sw $0, 4($1)
	addi $4, $0, 1000
	addi $2, $0, 32
	syscall
	addi $24, $23, 2048
	lw $25, 0($24)
	addi $10, $0, 0x109c22
	beq $25, $10, r_es
	j whileTrue2
#------VerificarKey-------#
r_es:
	addi $1, $0, 0
	addi $2, $0, 0
	addi $3, $0, 0
	addi $7, $0, 0
	addi $8, $0, 0
	addi $10, $0, 0
	addi $11, $0, 0
	addi $16, $0, 0 #verifica se o goomba estï¿½ indo para frente ou para trï¿½s
	addi $17, $0, 0 #guarda endereï¿½o do primeiro ponto do primeiro goomba pois o do $19 muda com o goomba 2
	addi $18, $0, 0 #contador do andar para frente e para trï¿½s do goomba
	addi $19, $0, 0 #guarda o primeiro endereï¿½o do goomba e ï¿½ ele que eu adiciono ou subtraiu para mover e passar como paraetro para desenhar na ela
	addi $20, $0, 0 #ele que ï¿½ o paramentro do for, quantas vezes tem que repetir
	addi $21, $0, 0 #contadpr para criar personagens na tela
	addi $22, $0, 0 
	addi $23, $0, 0 #guarda o valor da primeira posiï¿½ï¿½o da peaches ï¿½ ele que eu adiciono ou subtraiu para mover
	addi $24, $0, 0 #Pega o que o usuario digitou 
	addi $25, $0, 0
f_e:
	addi $20, $0, 8192
	addi $21, $0, 0
	addi $10, $0, 0x6aadb6
	lui $8, 0x1001
fundo_estrela:
	beq $20, $21,pause
	sw $10, 0($8)
	sw $10, 33000($8)
	addi $8, $8, 4
	addi $21, $21, 1
	j fundo_estrela
pause:
	lui $8, 0x1001
	addi $23, $8, 27156
	addi $10, $0, 0xff80ab
	j d_peaches2e
dpp:
