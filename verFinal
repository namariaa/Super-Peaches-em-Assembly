	lw $10, 33000($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $10, $0, 0xffffff #olho
	addi $8, $8, -3092
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	addi $10, $0, 0x000000
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 16
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $10, $0, 0xffffff
	addi $8, $8, -512
	lw $10, 33000($8)
	sw $10, 0($8)
	j goomba
