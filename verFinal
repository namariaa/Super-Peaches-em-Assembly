#------Fim dos personagens-------#
whileTrue:
j v_goomba
g:
	lui $8, 0x1001
	addi $19, $8, 24448
goomba:
	beq $3, 2, whileTrue
	addi $10, $0, 0x36180c #cor da cabeï¿½a para pisar
	addi $8, $19, 0
	addi $17, $19, 0 #usado para guardar primeiro ponto que muda por causa do goomba 2
	addi $20, $0, 3
