	addi $8, $8, -512
	sw $10, 0($8)
	beq $3, 1, verificarKey
	j g2
g2:
	addi $19, $19, -100
	j goomba2
goomba2:
	addi $10, $0, 0x36180c #cor da cabeï¿½a para pisar
