quadrado_npc: #fazer quadrado
	beq $20, $21, voltar
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, -508
	addi $21, $21, 1
	j quadrado_npc
#------Fim do mobs-------#
verificarKey:
	lui $24, 0x1001 #se chegar na parede vai para a fase2 de novo
	addi $24, $24, 26624
	lw $25, 0($24)
	addi $10, $0, 0xff80ab
	beq $25, $10, start_1
	lui $24, 0x1001 #se chegar na bandeira vai para a fase2
	addi $24, $24, 27588
	lw $25, 0($24)
	addi $10, $0, 0xff80ab
	beq $25, $10, m9 #rotulo para inciar a animação da bandeira subindo
	addi $10, $0, 0xd99866 #verificar se tem colisão com o primeiro goomba 
	addi $24, $23, 44
	lw $25, 0($24)
	beq $10, $25, morreu
	addi $10, $0, 0x422013
	addi $24, $23, -984
	lw $25, 0($24)
	beq $10, $25, morreu
	lui $1, 0xffff
	lw $24, 4($1) #se for diferente de 0 algo foi digitado
	beq $24, $0, whileTrue #se nï¿½o foi digitado nada
	addi $25, $0, 'd'
	beq $24, $25, moverFrente
	addi $25, $0, 'a'
	beq $24, $25, moverTras
	addi $25, $0, 'w'
	beq $24, $25, moverCima
	addi $25, $0, ' '
	beq $24, $25,whileTrue
moverFrente:
	sw $0, 4($1)#para ele caminhar só uma vez, colocar na região de memória do $1 o valor zero para não ficar caminhando infinitamente
	addi $23, $23, 12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar
moverTras:
	sw $0, 4($1)
	addi $23, $23, -12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar_invertida
moverCima:
	sw $0, 4($1)
	addi $23, $23, -16384 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar_pulo
moverBaixo:
	addi $24, $23, 14392
	lw $25, 0($24)
	addi $10, $0, 0x422013
	beq $25, $10, v_qualmorreu
	addi $24, $23, 14396
	lw $25, 0($24)
	beq $25, $10, v_qualmorreu
	addi $10, $0, 0xffffff
	addi $24, $23, 14396
	lw $25, 0($24)
	beq $25, $10, v_qualmorreu
	addi $23, $23, 16392 #Aumenta aqui para pular mais alto lembrando que esse ponto é 12 a mais que o moverCima
	addi $4, $0, 500 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
	j apagar_descer

v_qualmorreu:
	beq $3, $0, g_morreu 
	j g_morreu2
	
g_morreu:
	addi $3, $0, 1
	addi $4, $0, 300 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
	addi $23, $23, 16392
	addi $19, $19, 4
	addi $4, $0, 400 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
      	addi $3, $0, 5
	jal apagar_descer
	addi $3, $0, 1
	addi $4, $0, 600 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
	jal apagar_goomba
	j peaches
g_morreu2:
	addi $3,$0, 2
	addi $4, $0, 300 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
	addi $23, $23, 16392
	addi $19, $19, 4
	addi $4, $0, 400 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
      	addi $3, $0, 5
	jal apagar_descer
	addi $3, $0, 2
	addi $4, $0, 600 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
	jal apagar_goomba
	j peaches
#------Fim verificaï¿½ï¿½o-------#
peaches:
	addi $10, $0, 0xff80ab
	addi $8, $23, 0
	addi $20, $0, 12
	addi $21, $0,0
	jal barra_pink
	addi $8, $8, -1068
	addi $20, $0, 10
	addi $21, $0, 0
	jal barra_pink
	addi $10, $0, 0xd61e86
	addi $8, $8, -1060
	addi $20, $0, 8
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, -1052
	addi $21, $0, 0
	addi $20, $0, 6
	addi $10, $0, 0xff80ab
	jal barra_pink
	addi $8, $8, -32 #mangas 
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 28
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 476 #braï¿½os
	addi $10, $0, 0xe6c9b3
	addi $20, $0, 4
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, 16
	addi $20, $0, 2
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, -1568 #rosto
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_pink 
	addi $8, $8, -1048
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, 500 #olho
	addi $10,$0, 0x4d6df3
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 508
	addi $10,$0, 0x00b7ef
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $20, $0, 6 #comeï¿½a o cabelo a partir daqui
	addi $10, $0, 0xffc20e
	addi $21, $0, 0
	addi $8, $8, -1552
	jal barra_pink
	addi $20, $0, 4
	addi $21, $0, 0
	addi $8, $8, 480
	jal barra_pink
	addi $20, $0, 4
	addi $21, $0, 0
	addi $8, $8, 1008
	jal barra_pink
	addi $8, $8, 1004
	sw $10, 0($8)
	addi $8, $8, -512
	sw $10, 0($8)
	addi $8, $8, -1500 #franja
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, -2072 #coroa
	addi $10, $0, 0xa88e0a
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, -536 #fazer as pontas da coroa
	sw $10, 0($8)
	addi $8, $8, 8
	sw $10, 0($8)
	addi $8, $8, 12
	sw $10, 0($8)
	addi $10, $0, 0xc20c2d #joia coroa
	addi $8, $8, 504
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, 7148 #sapato
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 12
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	j whileTrue
barra_pink: #fazer quadrado
