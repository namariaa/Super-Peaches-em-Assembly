	addi $8, $8, 4
	sw $10, 0($8)
	j whileTrue3
#------Apagar flores-------#
bf:
	addi $20, $0, 3
	addi $21, $0, 0
	sw $0, 4($1)
	addi $8, $23, -2000 #desenhar  priemira bola
	addi $10, $0, 0xff8a14
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 516
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $8, $8, -516
	sw $10, 0($8)
	addi $8, $8, -512
	sw $10, 0($8)
	addi $10, $0, 0xfff811
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $8, $23, -2000
bola_fogo_loop:
	beq $20, $21, finish_clear #aqui verifico colisão
	addi $14, $8, 0
	jal apagar_bola_fogo
	addi $8, $14, 32
	jal bola_fogo
	addi $8, $14, 32
	addi $21, $21, 1
	j bola_fogo_loop
bola_fogo:
	addi $10, $0, 0xff8a14
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 516
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $8, $8, -516
	sw $10, 0($8)
	addi $8, $8, -512
	sw $10, 0($8)
	addi $10, $0, 0xfff811
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, -4
	sw $10, 0($8)
	addi $4, $0, 300
	addi $2, $0, 32
	syscall
	addi $10, $0, 0xfff811  #colisão
	lui $24, 0x1001
	addi $24, $24, 25884  
	lw $25, 0($24)  
	beq $25, $10, colisao_flor01
	addi $10, $0, 0xff8a14
	beq $25, $10, colisao_flor01
	addi $10, $0, 0xfff811  #colisão
	lui $24, 0x1001
	addi $24, $24, 25896 
	lw $25, 0($24)  
	beq $25, $10, colisao_flor01
	addi $10, $0, 0xff8a14
	beq $25, $10, colisao_flor01
	addi $10, $0, 0xfff811  #colisão
	lui $24, 0x1001
	addi $24, $24, 25956  
	lw $25, 0($24)  
	beq $25, $10, colisao_flor02
	addi $10, $0, 0xff8a14
	beq $25, $10, colisao_flor02
	addi $10, $0, 0xfff811  #colisão
	lui $24, 0x1001
	addi $24, $24, 25968
	lw $25, 0($24) 
	beq $25, $10, colisao_flor02
	addi $10, $0, 0xff8a14
	beq $25, $10, colisao_flor02
	jr $31
colisao_flor01:
	addi $3, $0, 1
	beq $20, $21, finish_clear
	jr $31
colisao_flor02:
	addi $3, $0, 2
	beq $20, $21, finish_clear
	jr $31
apagar_bola_fogo:
	addi $10, $0, 0xff8a14
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 516
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 508
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -516
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $10, $0, 0xfff811
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -4
	lw $10, 33000($8)
	sw $10, 0($8)
	jr $31
finish_clear:
	addi $10, $0, 0xff8a14
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 516
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 508
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -516
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $10, $0, 0xfff811
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -4
	lw $10, 33000($8)
	sw $10, 0($8)
	j whileTrue3
