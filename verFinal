cair_degrau:
	addi $10, $0, 0xc94a06
	sw $10, 0($24)
	addi $3, $0, 4
	jal apagar_yoshi_invertido
	addi $3, $0, 1
	addi $23, $23, 512
	j yoshi
teclado_impedido:
	lui $24, 0x1001
	addi $24, $24, 14336
	lw $25, 0($24)
	addi $10, $0, 0x22b14c
	beq $25, $10, start_4 #bata na parede volta
	addi $24, $23, 2576
	lw $25, 0($24)
	addi $10, $0, 0x0c6624
	bne $10, $25, onze
	addi $11, $0, 1
	addi $24, $23, -4084
	lw $25, 0($24)
	addi $10, $0, 0x000000
	beq $10, $25, morreu #morreu
	lui $1, 0xffff
	lw $24, 4($1)
	addi $25, $0, 'a'
	beq $24, $25, tras04
	addi $25, $0, 'w'
	beq $24, $25, pular04
	j whileTrue4
onze:
	addi $11, $0, 0
	j whileTrue4
frente04:
	sw $0, 4($1)
	j apagar_yoshi
tras04:
	sw $0, 4($1)
	addi $3, $0, 1
	j apagar_yoshi_invertido
pular04:
	sw $0, 4($1)
	addi $3, $0, 4
	jal apagar_yoshi
	addi $23, $23, -6144
	addi $4, $0, 100
	addi $2, $0, 32
	syscall
	jal yoshi
	addi $4, $0, 200
	addi $2, $0, 32
	syscall
	jal apagar_yoshi
	addi $3, $0, 1
	addi $24, $23,8720#caso for degrau
	addi $10, $0, 0x0c6624
	lw $25, 0($24)
	beq $25, $10, degrau
	addi $23, $23, 6152
	j yoshi
degrau: 
	addi $23, $23, 5640
	addi $24, $23, 3112
	lw $25, 0($24)
	addi $10,$0 ,0xc94a06
	beq $25, $10, degrau2
	addi $11, $0, 0
	addi $12, $0, 1
	j yoshi
degrau2:
	addi $23, $23, 0
	addi $11, $0, 0
	addi $12, $0, 1
	j yoshi
superpulo:
	sw $0, 4($1)
	lui $24, 0x1001
	addi $24, $24, 17528
	lw $25, 0($24)
	addi $10, $0, 0xe08e4f
	bne $25, $10, whileTrue4 #só funciona se ficar próximo
	addi $24, $24, -4
	lw $25, 0($24)
	bne $25, $10, whileTrue4
	addi $3, $0, 5
	jal apagar_yoshi
	addi $23, $23, -7164
	addi $4, $0, 100
	addi $2, $0, 32
	syscall
	jal yoshi
	addi $4, $0, 100
	addi $2, $0, 32
	syscall
	jal apagar_yoshi
	addi $3,$0, 1
	addi $23, $23, 12860
	j yoshi
