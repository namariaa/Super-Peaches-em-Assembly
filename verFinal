verificarKey3_2:
	lui $24, 0x1001
	addi $24, $24, 25508
	lw $25, 0($24)
	addi $10, $0, 0xa1102b
	beq $25, $10, c #se encostar no castelo
	lui $24, 0x1001 #se chegar na parede vai para a fase2 de novo
	addi $24, $24, 26624
	lw $25, 0($24)
	addi $10, $0, 0xffffff
	beq $25, $10, start_2
	addi $24, $23, -2512 #se morreu
	lw $25, 0($24)
	addi $10, $0, 0x9e0620
	beq $25, $10, morreu
	beq $3, 1, apagar_flor01 #se teve colisão com a flor 1
	beq $3, 2, apagar_flor02 #se teve colisão com a flor 2
	lui $1, 0xffff
	lw $24, 4($1) #se for diferente de 0 algo foi digitado
	beq $24, $0, whileTrue3 #se nï¿½o foi digitado nada
	addi $25, $0, 'd'
	beq $24, $25, moverFrente3
	addi $25, $0, 'a'
	beq $24, $25, moverTras3
	addi $25, $0, ' '
	beq $24, $25,bf
#------VerificarKey-------#
apagar_flor01:
