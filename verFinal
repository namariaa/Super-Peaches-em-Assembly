voltar_bandeira:
	jr $25
#---------Teclado---------#	
c_pos: #confeir se ele vai cair do degrau
	beq $12, $0, verificarKey4
	beq $13, 1, conferir_caiu
	j verificarKey4
conferir_caiu:
	beq $12, $0, verificarKey4
	addi $10, $0, 0xc94a06
	addi $24, $23, 3068
	lw $25, 0($24)
	beq $25, $10, cair_degrau
verificarKey4:
	beq $11, 1, teclado_impedido
	lui $24, 0x1001 #se chegar na bandeira vai para a fase2
	addi $24, $24, 22468
	lw $25, 0($24)
	addi $10, $0, 0x22b14c
	beq $25, $10, m94 #rotulo para inciar a animação da bandeira subindo
	addi $10, $0, 0xb4b4b4
	beq $25, $10, m94 #rotulo para inciar a animação da bandeira subindo
	lui $24, 0x1001
	addi $24, $24, 17536
	lw $25, 0($24)
	addi $10, $0, 0xe08e4f
	beq $25, $10, queda #cair no precipicoo
	addi $24, $23, 2576
	lw $25, 0($24)
	addi $10, $0, 0x0c6624
	beq $10, $25, teclado_impedido #bateu degrau
	addi $24, $23, -4084
	lw $25, 0($24)
	addi $10, $0, 0x000000
	beq $10, $25, morreu #morreu
	lui $24, 0x1001
	addi $24, $24, 14336
	lw $25, 0($24)
	addi $10, $0, 0x22b14c
	beq $25, $10, start_4 #bata na parede volta do inicio 
	lui $1, 0xffff
	lw $24, 4($1)
	addi $25, $0, 'd'
	beq $24, $25, frente04
	addi $25, $0, 'a'
	beq $24, $25, tras04
	addi $25, $0, 'w'
	beq $24, $25, pular04
	addi $25, $0, ' '
	beq $24, $25, superpulo
	j whileTrue4
queda:
	addi $15, $0, 7
	addi $16, $0, 0
	j cair 
cair:
	beq $15, $16, jump
	addi $16, $16, 1
	addi $3, $0, 5
	jal apagar_yoshi
	addi $23, $23, 2052
	addi $4, $0, 200
	addi $2, $0, 32
	syscall
	jal yoshi
	addi $3, $0, 1
	j cair
jump:
	addi $15, $0, 5
	addi $16, $0, 0
	addi $3, $0, 5
	jal apagar_yoshi
	addi $4, $0, 40
	addi $2, $0, 32
	syscall
animar_jump:
	beq $15, $16, ay
	addi $16, $16, 1
	addi $3, $0, 5
	jal apagar_yoshi_mortinho
	addi $23, $23, -3072
	jal yoshi_mortinho
	j animar_jump
ay:
	addi $15, $0, 8
	addi $16, $0, 0	
	jal apagar_yoshi_mortinho
	addi $23, $23, 8
