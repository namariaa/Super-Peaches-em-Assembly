	addi $8, $8, 12
	sw $10, 0($8)
	addi $10, $0, 0xc20c2d #joia coroa
	addi $8, $8, 504
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
	addi $8, $8, 7148 #sapato
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
