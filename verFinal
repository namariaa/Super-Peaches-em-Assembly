peaches_descer:
	addi $10, $0, 0xff80ab
	addi $8, $23, 0
	addi $20, $0, 12
	addi $21, $0,0
	jal barra_pink
	addi $8, $8, -1068
	addi $20, $0, 10
	addi $21, $0, 0
	jal barra_pink
	addi $10, $0, 0xd61e86
	addi $8, $8, -1060
	addi $20, $0, 8
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, -1052
	addi $21, $0, 0
	addi $20, $0, 6
	addi $10, $0, 0xff80ab
	jal barra_pink
	addi $8, $8, -32 #mangas 
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 28
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 476 #braï¿½os
	addi $10, $0, 0xe6c9b3
	addi $20, $0, 4
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, 16
	addi $20, $0, 2
	addi $21, $0, 0
	jal barra_pink
	addi $8, $8, -1568 #rosto
