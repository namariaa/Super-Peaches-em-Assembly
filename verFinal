verificarKey3:
	lui $24, 0x1001 #se chegar na parede vai para a fase2 de novo
	addi $24, $24, 26624
	lw $25, 0($24)
	addi $10, $0, 0xff80ab
	beq $25, $10, start_2
	lui $24, 0x1001 #se morreu
	addi $24, $24, 24836
	lw $25, 0($24)
	addi $10, $0, 0xe6c9b3
	beq $25, $10, morreu
	lui $24, 0x1001 #se morreu
	addi $24, $24, 24908
	lw $25, 0($24)
	addi $10, $0, 0xe6c9b3
	beq $25, $10, morreu
	lui $1, 0xffff
	lw $24, 4($1) #se for diferente de 0 algo foi digitado
	beq $24, $0, whileTrue3 #se nï¿½o foi digitado nada
	addi $25, $0, 'd'
	beq $24, $25, moverFrente3
	addi $25, $0, 'a'
	beq $24, $25, moverTras3
	addi $25, $0, 'w'
	beq $24, $25, moverCima3
	addi $25, $0, ' '
	beq $24, $25,whileTrue3
moverFrente3:
	sw $0, 4($1)#para ele caminhar só uma vez, colocar na região de memória do $1 o valor zero para não ficar caminhando infinitamente
	addi $23, $23, 12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar3
moverTras3:
	sw $0, 4($1)
	addi $23, $23, -12 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	j apagar_invertida3
moverCima3:
	sw $0, 4($1)
	addi $23, $23, -3072 #$23 guarda o valor da posiï¿½ï¿½o do priemiro ponto
	jal apagar_pulo3
moverBaixo3:
	addi $23, $23, 3072 #Aumenta aqui para pular mais alto lembrando que esse ponto é 12 a mais que o moverCima
	addi $4, $0, 550 #os trï¿½s proximos comandos ï¿½ para sleep. Quanto maior mais ela fica no ar
      	addi $2, $0, 32
      	syscall
	jal apagar_descer3
