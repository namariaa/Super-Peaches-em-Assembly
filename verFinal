.text

start_1:
	addi $1, $0, 0
	addi $2, $0, 0
	addi $3, $0, 0
	addi $8, $0, 0
	addi $10, $0, 0
	addi $11, $0, 0
	addi $16, $0, 0 
	addi $17, $0, 0 
	addi $18, $0, 0 
	addi $19, $0, 0 
	addi $20, $0, 0 
	addi $21, $0, 0 
	addi $22, $0, 0 
	addi $23, $0, 0 
	addi $24, $0, 0 
	addi $25, $0, 0 
	j cen01
cen01:   
	addi $9, $0, 1 #para animação da morte
	lui $8, 0x1001
	addi $20, $0, 2048
	addi $10, $0, 0x029DF0
cor1:
	beq $21, $20, m2
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $21, $21, 1
	addi $8, $8, 4
	j cor1
m2:
	addi $21, $0, 0
	addi $10, $0, 0x059EF0
cor2:
	beq $20, $21, m3
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8, 4
	addi $21, $21, 1
	j cor2
m3:
	addi $10, $0, 0x05A0F3
	addi $21, $0, 0
cor3:
	beq $20, $21, m4
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8, 4
	addi $21, $21, 1
	j cor3
m4:
	addi $21, $0, 0
	addi $10, $0, 0x109A0D
	addi $20, $0, 512
chao_verde:
	beq $20, $21, m5
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $21, $21, 1
	addi $8, $8, 4
	j chao_verde
m5:
	addi $20, $0, 2048
	addi $21, $0, 0
	addi $10, $0, 0xDF9126	
chao_marrom:
	beq $20, $21, m6
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8, 4
	addi $21, $21, 1
	j chao_marrom
m6:
	lui $8, 0x1001
	addi $10, $0, 0xA3510C
	addi $21, $0, 0
	addi $8, $8, 27000
	addi $20, $0, 50
detalhes:
	beq $20, $21, m8
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $21, $21, 1
	addi $8, $8, 200
	j detalhes
m8:
	lui $8, 0x1001
	addi $10, $0, 0x089506
	addi $21, $0, 0
	addi $8, $8, 26600
	addi $20, $0, 50
mato:
	beq $20, $21, mastro
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8,4
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8,4
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8,4
	sw $10, 0($8)
	sw $10, 33000($8) # cï¿½pia carbono
	addi $8, $8,4
	addi $21, $21, 1
	addi $8, $8, 4
	j mato
mastro:
	lui $8, 0x1001
	addi $8, $8, 9156
	addi $20,$0, 40
	addi $21, $0, 0
	addi $10, $0, 0x8f888f
	jal linha_mastro
	addi $10, $0, 0x8c032e #bandeira coraï¿½ï¿½o
	addi $8, $8, -2556
	sw $10, 0($8)
	addi $8,$8, -508
	sw $10, 0($8)
	addi $8, $8, -508
	sw $10, 0($8)
	addi $8, $8, 516
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, 516
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, -512
	sw $10, 0($8)
	addi $8, $8, -512
	sw $10, 0($8)
	addi $8, $8, -516
	sw $10, 0($8)
	addi $8, $8, 4
	sw $10, 0($8)
	addi $8, $8, 508
	sw $10, 0($8)
	addi $8, $8, 512
	sw $10, 0($8)
