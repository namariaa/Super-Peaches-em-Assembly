detalhe_koopa_apaga: #fazer linha
	beq $20, $21, voltar
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	addi $21, $21, 1
	j detalhe_koopa_apaga
quadrado_npc2_apaga: #fazer quadrado
	beq $20, $21, voltar
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -508
	addi $21, $21, 1
	j quadrado_npc2_apaga
