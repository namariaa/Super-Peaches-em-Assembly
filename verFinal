	addi $21, $0, 0
	jal lin_hff
	addi $8, $8, -512
	sw $10, 0($8) 
	addi $8, $8, 4
	sw $10, 0($8) 
	addi $8, $8, -512
	sw $10, 0($8) 
	addi $8, $8, -512
	sw $10, 0($8) 
