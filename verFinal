v_koopa: #para ver se já apareceu 3 koopas
	addi $20, $0, 2
	beq $18, $20, clear_end
	lui $24, 0x1001 #verificar s elee chegou na parede
	addi $24, $24, 26116
	lw $20, 0($24) 
	addi $25, $0, 0xd99866
	beq $20, $25, r_koopa
	j moverKoopa
clear_end: #pois quando ompleta os 3 koopas apararecendo fica um fixo
	addi $8, $11, 0
	addi $10, $0, 0x12b029
	addi $21, $0, 0
	addi $20, $0, 6
	jal quadrado_npc2_apaga
	addi $8, $8, -1560
	addi $21, $0, 0
	addi $20, $0, 6
	jal detalhe_koopa_apaga
	addi $8, $8, -2556
	addi $21, $0, 0
	addi $20, $0, 5
	jal detalhe_koopa_apaga
	addi $8, $8, -2556
	addi $21, $0, 0
	jal detalhe_koopa_apaga
	addi $8, $8, -2556
	addi $21, $0, 0
	jal detalhe_koopa_apaga
	addi $8, $8, -2044
	addi $21, $0, 0
	addi $20, $0, 4
	jal detalhe_koopa_apaga
	addi $10, $0, 0xffffff #linha branca
	addi $8, $8, -3092
	addi $20, $0, 7
	addi $21, $0, 0
	jal detalhe_koopa_apaga
	addi $8, $8, -508 #pernas
	addi $10, $0, 0xd99866
	addi $20, $0, 5
	addi $21, $0, 0
	jal quadrado_npc2_apaga
	addi $8, $8, -12
	addi $10, $0, 0x109A0D
	addi $21, $0, 0
	addi $20, $0, 2
	jal detalhe_koopa_apaga
	addi $10, $0, 0xd99866 #cabeï¿½a
	addi $21, $0, 1
	addi $20, $0, 3
	addi $8, $8, -3092
	jal quadrado_npc2_apaga
	addi $21, $0, 0
	addi $8, $8, -1036
	jal quadrado_npc2_apaga
	addi $21, $0, 0
	addi $8, $8, -1036
	jal quadrado_npc2_apaga
	addi $8, $8, 496
	addi $21, $0, 0
	addi $20, $0, 4
	jal detalhe_koopa_apaga
	addi $8, $8, -508
	lw $10, 33000($8)
	sw $10, ($8)
	addi $10, $0, 0xffffff #olho
	addi $8, $8, -2048
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 4
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -4
	addi $10, $0, 0x000000
	lw $10, 33000($8)
	sw $10, ($8)
	j v_n
r_koopa:
	addi $18, $18, 1 #vê se já apareceu 3 koopa
	jal apagarKoopa2 #precisa de outro pq preciso dá jr em vez de jump
	addi $19, $11, 0
	j koopa
moverKoopa:
	addi $19, $19, -24
	addi $4, $0, 200
	addi $2, $0, 32
	syscall
	j apagarKoopa
apagarKoopa:
	addi $8, $19, 0
	addi $8, $8, 24
