	addi $10, $0, 0xcfe9f7
	addi $17, $0, 1
	j neve 
neve:
	lui $8, 0x1001
	addi $8, $8, 1100
	jal floco
	addi $8, $8, 4100
	jal floco
	addi $8, $8, 548
	jal floco
	addi $8, $8, 580
	jal floco
	addi $8, $8, -256
	jal floco
	addi $8, $8, 600
	jal floco
	addi $8, $8, 1000
	jal floco
	addi $8, $8, 60
	jal floco
	addi $8, $8, 1020
	jal floco
	addi $8, $8, 620
	jal floco
	addi $8, $8, 528
	jal floco
	addi $8, $8, 160
	jal floco
