	addi $8, $8, -512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -1552 #franja
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -2044 #coroa
	addi $10, $0, 0xa88e0a
	addi $20, $0, 6
	addi $21, $0, 0
	jal barra_apagar_invertida3
	addi $8, $8, -536 #fazer as pontas da coroa
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 8
	lw $10, 33000($8)
	sw $10, 0($8)
