barra_apagar_pular3:
	beq $20, $21, voltar
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, 512
	addi $8, $8, 0
	lw $10, 33000($8)
	sw $10, 0($8)
	addi $8, $8, -508
	addi $21, $21, 1
	j barra_apagar_pular3

